<resources>
    <!-- Base application NoActionBar. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:colorBackground">@color/colorPrimary</item>
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:textColorHint">@color/colorTextDisable</item>
    </style>

    <style name="TextColorTPrimary28B">
        <item name="android:textSize">28dp</item>
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextColorTPrimaryPt16B">
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextColorTPrimaryPt14B">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/colorTextPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextColorTPrimaryPt12">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="TextColorTPrimaryPt10">
        <item name="android:textSize">10dp</item>
        <item name="android:textColor">@color/colorTextPrimary</item>
    </style>

    <style name="TextSelectColorTPrimary_ColorTDisablePt14">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/select_color__color_t_primary__color_t_disable</item>
    </style>

</resources>